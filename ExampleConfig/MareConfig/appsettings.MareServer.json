{
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"MareSynchronosServer": "Information"
		},
		"File": {
			"Files": [ {"Path": "Server.<date>.log"} ]
		}
	},
	"MareSynchronos": {
		"ShardName": "Main",
		"MetricsPort": 5901
	},
	"IpRateLimiting": { },
	"IPRateLimitPolicies": { },
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:6000"
			},
			"NamedPipeEndpoint": {
				"Url": "http://unix:/run/mare-server/server.sock"
			}
		}
	}
}
