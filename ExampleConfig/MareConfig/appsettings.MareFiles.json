{
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"MareSynchronosStaticFilesServer": "Information"
		},
		"File": {
			"Files": [ {"Path": "Files.<date>.log"} ]
		}
	},
	"MareSynchronos": {
		"ShardName": "Files",
		"IsDistributionNode": true,
		"MetricsPort": 5903,
		"MainServerAddress": "http://127.0.0.1:6000/",
		"CacheDirectory": "/mnt/lopcache/",
		"CacheSizeHardLimitInGiB": 50,
		"MinimumFileRetentionPeriodInDays": 7,
		"UnusedFileRetentionPeriodInDays": 30,
		"UseColdStorage": false,
		"ColdStorageDirectory": "/mnt/coldstorage/",
		"ColdStorageSizeHardLimitInGiB": 500,
		"ColdStorageUnusedFileRetentionPeriodInDays": 90,
		"UseXAccelRedirect": true,
		"UseSSI": true,
		"CdnShardConfiguration": []
	},
	"IpRateLimiting": {},
	"IPRateLimitPolicies": {},
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:6200"
			},
			"NamedPipeEndpoint": {
				"Url": "http://unix:/run/mare-files/server.sock"
			}
		}
	}
}
