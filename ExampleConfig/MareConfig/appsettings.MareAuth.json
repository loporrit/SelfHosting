{
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"MareSynchronosAuthService": "Information"
		},
		"File": {
			"Files": [ {"Path": "Auth.<date>.log"} ]
		}
	},
	"MareSynchronos": {
		"ShardName": "Auth",
		"MetricsPort": 5904,
		"WellKnown": ""
	},
	"IpRateLimiting": { },
	"IPRateLimitPolicies": { },
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:6001"
			},
			"NamedPipeEndpoint": {
				"Url": "http://unix:/run/mare-auth/server.sock"
			}
		}
	}
}
