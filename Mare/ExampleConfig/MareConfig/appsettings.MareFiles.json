{
    "ConnectionStrings": {
        "DefaultConnection": "Host=/var/run/postgresql;Port=5432;Database=mare;Username=mare;Keepalive=15;Minimum Pool Size=4;Maximum Pool Size=8;No Reset On Close=true;Max Auto Prepare=50;Enlist=false"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "MareSynchronosStaticFilesServer": "Information",
            "MareSynchronosShared": "Information",
            "System.IO": "Information"
        },
        "File": {
            "BasePath": "logs",
            "FileAccessMode": "KeepOpenAndAutoFlush",
            "FileEncodingName": "utf-8",
            "DateFormat": "yyyMMdd",
            "MaxFileSize": 104857600,
            "Files": [
                {
                    "Path": "<date:yyyy>/<date:MM>/<date:dd>/mare-files-<date:HH>-<counter:0000>.log"
                }
            ]
        }
    },
    "MareSynchronos": {
        "DbContextPoolSize": 512,
        "ShardName": "Files",
        "MetricsPort": 6250,
        "ForcedDeletionOfFilesAfterHours": -1,
        "CacheSizeHardLimitInGiB": 50,
        "UnusedFileRetentionPeriodInDays": 30,
        "CacheDirectory": "/mnt/marecache/",
        "RemoteCacheSourceUri": "",
        "MainServerAddress": "http://127.0.0.1:6000",
        "RedisConnectionString": "127.0.0.1",
        "MainFileServerAddress": "",
        "Jwt": "fdsafdsafdsafdsa",
        "UseColdStorage": false,
        "IsDistributionNode": true,
        "CdnFullUrl": "https://example.sync"
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://+:6200"
            },
            "NamedPipeEndpoint": {
                "Url": "http://unix:/var/run/mare-files/server.sock"
            }
        }
    },
    "IpRateLimiting": {},
    "IPRateLimitPolicies": {}
}
